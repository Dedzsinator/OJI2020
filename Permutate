#include<iostream>
#include <fstream>
#include<cstdlib>
using namespace std;

ofstream g("out.out");

int s[100], teto = 1;
int n;

bool check(int s[100], int x) {
	for (int i = 1; i <= x - 1; i++) {
		for (int j = i + 1; j <= x; j++) {
			if (s[i] == s[j]) {
				return false;
			}
		}
	}
	return true;
}

bool solve(int s[100], int y, int x) {
	if (y == x) {
		return true;
	}
	return false;
}

void readout(int s[100], int x) {
	for (int i = 1; i <= x; i++) {
		g << s[i] << " ";
	}
	g << endl;
}

void perm(int s[100], int y, int x) {
	//g << "asd";
	for (int i = 1; i <= x; i++) {
		s[y] = i;
		if (check(s, x)) {
			if (solve(s, y, x)) {
				readout(s, x);
			}
			else {
				perm(s, y++, x);
			}
		}
	}
}

int main() {
	cin >> n;
	perm(s, teto, n);
	return 0;
}
